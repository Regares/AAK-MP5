#include <stdio.h>
#include <math.h>

int main()
{
    float x1, y1, x2, y2, res;
    printf("Введите координаты первой точки через пробел: ");
    scanf("%f%f", &x1, &y1);
    printf("Введите координаты второй точки через пробел: ");
    scanf("%f%f", &x2, &y2);
    res = sqrt(pow((x2 - x1), 2) + pow((y2 - y1), 2));
    printf("Расстояние между этими точками равно: %f", res);
    return 0;
}

#include <stdio.h>
#include <math.h>

int main()
{
    float x1, y1, x2, y2, res1, res2;
    printf("Введите координаты первой точки через пробел: ");
    scanf("%f%f", &x1, &y2);
    printf("Введите координаты второй точки через пробел: ");
    scanf("%f%f", &x2, &y2);
    res1 = pow((x2 - x1), 2) + pow((y2 - y1), 2);
    if (res1 == 0)
    {
        printf("Расстояние между этими точками равно: 0.0000000");
    }
    else
    {
        res2 = sqrt(res1);
        printf("Расстояние между этими .точками равно: %f", res2);
    }
    return 0;
}
